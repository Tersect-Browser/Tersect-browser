{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/"],
        "cache": true
      },
      "lint": {
        "dependsOn": ["^lint"]
      },
      "deploy": {
        "dependsOn": ["^build"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      }
    }
  }
